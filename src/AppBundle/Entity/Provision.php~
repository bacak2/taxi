<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="provision")
 */
class Provision
{
    /**
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     * @ORM\Column(type="integer")
     */
    private $id;

    /**
     * @ORM\Column(type="decimal", scale=2, precision=4)
     */
    private $ownCards;

    /**
     * @ORM\Column(type="decimal", scale=2, precision=4)
     */
    private $eVoucher;

    /**
     * @ORM\Column(type="decimal", scale=2, precision=4)
     */
    private $voucher919;
}